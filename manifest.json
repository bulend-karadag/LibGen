{
    "name": "LibGen Search & Download",
    "description": "Search for ebooks and pdf in LibGen library.",
    "version": "0.2.0",
    "manifest_version": 3,
    "icons": {
         "32": "icon.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "32": "icon.png"
        }
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
      "http://gen.lib.rus.ec/*",
      "http://libgen.is/*",
      "http://libgen.gs/*",
      "http://libgen.rs/*",
      "http://libgen.lc/*",
      "https://libgen.rocks/*"
    ]

}

